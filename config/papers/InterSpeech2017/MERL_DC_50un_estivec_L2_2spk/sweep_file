ivec10
trainer.cfg task_2spk ivecs trainivec10
trainer.cfg trainer init_file /esat/spchtemp/scratch/jzegers/Nabu-SS2.0/Default17_MERL_DC_ivec_sweep/ivec10/full/model/network.ckpt
validation_evaluator.cfg task_2spk ivecs devivec10
test_evaluator.cfg task_2spk ivecs testivec10
test_evaluator.cfg task_2spk_tr ivecs trainivec10
test_evaluator.cfg task_2spk_dev ivecs devivec10
postprocessor.cfg task_2spk tv_dim 10
postprocessor.cfg task_2spk lda False
postprocessor.cfg task_2spk_tr tv_dim 10
postprocessor.cfg task_2spk_tr lda False
postprocessor.cfg task_2spk_dev tv_dim 10
postprocessor.cfg task_2spk_dev lda False

